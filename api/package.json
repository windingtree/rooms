  },
  "homepage": "https://github.com/windingtree/rooms#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.2.6",
    "@vercel/node": "^1.8.2",
    "aws-sdk": "^2.854.0",
    "axios": "^0.21.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "jimp": "^0.16.1",
    "jose": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
